function IsJsonString(text) {
    try {
        JSON.parse(text);
		if (text.indexOf('{') ==0 || text.indexOf('[') ==0) {
				return true;
			}else{
				return false;
			}
    } catch (e) {
        return false;
    }
}


if(metadados_resumo != null){
var arr = metadados_resumo.split('||||:||||');
var valor = '';
var jsons = '';


for(var i=1; i&lt;arr.length; i+=1){
	valor = arr[i].split('||||')[0];
	if(IsJsonString(valor)){
		var ini = metadados_resumo.indexOf(valor)-4;
		var end = ini + 8 +valor.length;
		valor = replace(valor, '"', '||||');
		var text = substr(metadados_resumo, 0, ini) + valor + substr(metadados_resumo, end);
		metadados_resumo = text;
	}		
}
}


//////////////
if(metadados != null){
arr = metadados.split('||||:||||');

for(var i=1; i&lt;arr.length; i+=1){
