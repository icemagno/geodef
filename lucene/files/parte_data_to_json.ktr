        <class_source>//import net.sf.json.JSONObject;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    Object[] r = getRow();
 
    if (r == null) {
        setOutputDone();
        return false;
    }
 
    if (first){
        first = false;
    }
 
    r = createOutputRow(r, data.outputRowMeta.size());
     
	
	String link_simbolo = "";
	
	try{

		String color = get(Fields.In, "mapcolor").getString(r).replace("#", "_");
		String simbolo = get(Fields.In, "mapsymbol").getString(r);
		link_simbolo = "http://apolo.defesa.mil.br/SvgService/symbol.svg?symbol="+simbolo+"&amp;color="+color ;

	}catch(Exception e) {
	
	}

	get(Fields.Out, "link_simbolo").setValue(r, link_simbolo);

 
    // Send the row on to the next step.
    putRow(data.outputRowMeta, r);
 
    return true;
}</class_source>
