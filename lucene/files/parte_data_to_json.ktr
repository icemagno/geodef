    if (r == null) {
        setOutputDone();
        return false;
    }
 
    if (first){
        first = false;
    }
 
    r = createOutputRow(r, data.outputRowMeta.size());
     
	String info = get(Fields.In, "labels_resumo").getString(r);
	String resumo = get(Fields.In, "metadados_resumo").getString(r);
	Long qtd = get(Fields.In, "qtd_metadados_ordenados").getInteger(r);
	String metadados = "";
	String fields = "";
	String metadadosResumo = "";


if(info != null &amp;&amp; info !=""){

	 
	String arr[] = resumo.split(",");
	String arr_label[] = info.split(",");
	

	
       
	for(int i=1;i&lt;arr.length;i++){
		String col = arr[i];


        if(col == "geom_json" || col == "geom_polygon"|| col == "geom_point" || col == "shapefile_id" || col == "geom"){}		
		else{
			String x = get(Fields.In, col).getString(r);
			if(x != null ){
				x = x.trim().replaceAll("\\r\\n|\\r|\\n", " ");
			}			
	
			if(resumo.contains(","+col.toLowerCase()+",")){

			if(i > qtd){
				metadadosResumo += "||||"+arr_label[i]+"||||:{||||order||||:null,||||value||||:||||"+x+"||||},";
			} else{
				metadadosResumo += "||||"+arr_label[i]+"||||:{||||order||||:"+i+",||||value||||:||||"+x+"||||},";
			}
			
		
		} 

		
	



			
        }

    }
}

